<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated default -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="pipelineproject.presentation.PipelineprojectModelWizardID"
            name="%_UI_PipelineprojectModelWizard_label"
            class="pipelineproject.presentation.PipelineprojectModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PipelineprojectModelFile.gif">
         <description>%_UI_PipelineprojectModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated default -->
      <editor
            id="pipelineproject.presentation.PipelineprojectEditorID"
            name="%_UI_PipelineprojectEditor_label"
            icon="icons/full/obj16/PipelineprojectModelFile.gif"
            extensions="pipelineproject"
            class="pipelineproject.presentation.PipelineprojectEditor"
            contributorClass="pipelineproject.presentation.PipelineprojectActionBarContributor">
      </editor>
   </extension>

</plugin>
